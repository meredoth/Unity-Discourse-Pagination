{
    "manifest_version": 3,
    "name": "Unity Discourse Pagination",
    "version": "1.0",
    "description": "Add pagination buttons to Unity discourse forum",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/pagination.js"
            ],
            "matches": [
                "https://discussions.unity.com/t/*"
            ]
        }
    ]
}